       <div class="content">
        <div class="row">
          <div class="col-md-12">
            <div class="card">
              <div class="card-header">
                <h4 class="card-title">Received Submissions</h4>
              </div>
              <div class="card-body">
                <div class="table-responsive">
                  <table class="table">
                    <thecad class=" text-primary">
                      <th class="text-left">
                        Enrollment No.
                      </th>

                      <th class="text-center">
                        Assignment ID
                      </th>

                      <th class="text-center">
                        Submitted On
                      </th>
                    </thead>
                    <tbody>
                      <tr>
                        <td>
                          <% uploads.forEach(function(upload){ %>
                          <li>
                            <strong>
                              <%= upload.roll %>
                            </strong>
                          </li>
                          <% }) %>
                        </td>
                       
                        <td class="text-center">
                          <% uploads.forEach(function(upload){ %>                          
                              <%= upload.id%>
                              <br>                            
                          <% }) %>
                        </td> 

                        <td class="text-center">
                          <% uploads.forEach(function(upload){ %>                          
                              <%= upload.date.toDateString()%>
                              <br>                            
                          <% }) %>
                        </td>
                       </tr>
                      
                    </tbody>
                  </table>
                </div>
              </div>
            </div>

  
          <div class="col-md-12">
            <div class="card card-user" style="margin: 80px">
              <div class="card-header">
                <h5 class="card-title">Assignment & Information Control</h5>
              </div>
              <div class="card-body">

          <form action="/addassignment" method="POST">
               <div class="row pr-2 pl-2">
                    <div class="col-md-6">
                      <div class="form-group">
                        <label for="Name">Name of Subject</label>
                        <input type="text" class="form-control" placeholder="Enter Subject" name="subject">
                      </div>
                    </div>
                    
                  
                 
                    <div class="col-md-6">
                      <div class="form-group">
                        <label>Due On</label>
                        <input type="date" class="form-control" placeholder="Enter Due Date" name="due">
                      </div>
                    </div>
                     <div class="col-md-4">
                      <div class="form-group">
                        <label>Credit Score</label>
                        <input type="number" class="form-control" placeholder="Enter Credit" min="1" max="6" name="credit">
                      </div>
                    </div>
                   </div>
                  
                  <div class="row">
                  <div class="update ml-auto mr-auto">
                    <form action="/assignments" method="post"><input type="file" class="btn btn-danger btn-sm btn-round pl-auto pr-auto">                       
                    </div>
                  </div>
                  <div class="row">
                    <div class="update ml-auto mr-auto">
                      <button type="submit" class="btn btn-warning btn-round">Upload Files</button>
                    </div>
              </div>
                </form>
              </form>
              </div>
                    </div>
                  </div>
            </div>
          </div>
        </div>
